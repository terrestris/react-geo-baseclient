{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "config/jest/spec/*.js",
      "node_modules/**/*.ts",
      "node_modules",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": [true, "never-prefix"],
    "jsx-no-lambda": false,
    "jsx-wrap-multiline": false
  }
}
